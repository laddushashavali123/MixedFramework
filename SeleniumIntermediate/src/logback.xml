		<connectionSource class="ch.qos.logback.core.db.DataSourceConnectionSource">
				<dataSource class="com.mysql.jdbc.jdbc2.optional.MysqlDataSource">
			<serverName>http://php4dvd.com.ua/openserver/</serverName>
